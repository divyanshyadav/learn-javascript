{
	"env": {
		"browser": true,
		"es6": false,
		"node": true,
		"jest": true
	},
	"extends": [],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": ["immutable", "babel"],
	"rules": {
		"immutable/no-mutation": 0,
		"immutable/no-let": 2,
		"indent": ["error", 4],
		"linebreak-style": 0
	}
}
